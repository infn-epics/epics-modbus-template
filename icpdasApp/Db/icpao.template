
record(ao, "$(P):$(R):$(ID)")
{
    field(DESC, "AO$(REGOFFSET)")
    field(DTYP, "asynFloat64")
    field(OUT,  "@asynMask($(PORT) $(REGOFFSET) 16))INT16")
    field(SCAN, "I/O Intr")
}
record(ai, "$(P):$(R):$(ID)_RB")
{
    field(DESC, "AO$(REGOFFSET)RB")
    field(DTYP, "asynFloat64")
    field(INP,  "@asynMask($(PORT) $(REGOFFSET) 16))INT16")
    field(SCAN, "I/O Intr")
}
